<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Dialog
        id="addParkingLotDialog"
        title="Add Parking Lot"
        >
        <VBox>
            <Label
                id="labelParkingLotNo"
                text="Parking Lot No"
                />
            <Input
                id="parkingLotNoInput"
                placeholder="Enter Parking Lot No"
                />
            <Label
                id="labelProcessType"
                text="Process Type"
                />
            <Input
                id="processTypeInput"
                placeholder="Enter Process Type"
                />
        </VBox>
        <Toolbar>
            <ToolbarSpacer />
            <Button
                text="Save"
                press="onSavePress" />
            <Button
                text="Cancel"
                press="onCancelPress" />
        </Toolbar>
    </Dialog>
</core:FragmentDefinition>
